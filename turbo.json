{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    }
  },
  "ui": "stream",
  "globalEnv": [
    "__NEXT_PROCESSED_ENV",
    "AUTH_TOKEN",
    "AUTH_SECRET",
    "AUTH_TRUST_HOST",
    "AUTH_TWITTER_ID",
    "AUTH_TWITTER_SECRET",
    "AUTH_URL",
    "BLOB_READ_WRITE_TOKEN",
    "CI_ENV",
    "COREPACK_ENABLE_STRICT",
    "DATABASE_URL",
    "DATABASE_URL_UNPOOLED",
    "GA_MEASUREMENT_ID",
    "GA_PROTOCOL_SECRET",
    "GA_STREAM_ID",
    "GITHUB_PAT",
    "MY_GITHUB_PAT",
    "NEXTAUTH_SECRET",
    "NEXT_PUBLIC_GA_MEASUREMENT_ID",
    "NEXT_PUBLIC_MEASUREMENT_PROTOCOL_SECRET",
    "NEXT_PUBLIC_GA_PROTOCOL_SECRET",
    "NEXT_PUBLIC_GA_STREAM_ID",
    "NPM_RC",
    "npm_config_user_agent",
    "NO_COLOR",
    "NODE_ENV",
    "NPM_TOKEN",
    "OPEN_AI_KEY",
    "PGDATABASE",
    "PGHOST",
    "PGHOST_UNPOOLED",
    "PGPASSWORD",
    "PGUSER",
    "PORT",
    "POSTGRES_DATABASE",
    "POSTGRES_HOST",
    "POSTGRES_PASSWORD",
    "POSTGRES_PRISMA_URL",
    "POSTGRES_URL",
    "POSTGRES_URL_NON_POOLING",
    "POSTGRES_URL_NO_SSL",
    "POSTGRES_USER",
    "REDIS_URL",
    "TWITTER_ACCESS_TOKEN",
    "TWITTER_ACCESS_TOKEN_SECRET",
    "TWITTER_API_KEY",
    "TWITTER_API_KEY_SECRET",
    "TWITTER_BEARER_TOKEN",
    "VERCEL_ENV",
    "VERCEL_GIT_PROVIDER",
    "VERCEL_GIT_REPO_OWNER",
    "VERCEL_GIT_REPO_SLUG",
    "VERCEL_URL"
  ]
}
