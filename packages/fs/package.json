{
  "name": "@portfolio/fs",
  "version": "4.1.0",
  "description": "convenient fs helpers",
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ],
        "fs": [
          "dist/fs/index.d.ts"
        ],
        "types": [
          "dist/types/index.d.ts"
        ]
      }
    }
  },
  "license": "MIT",
  "source": "src/index.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ],
      "fs": [
        "dist/fs/index.d.ts"
      ],
      "types": [
        "dist/types/index.d.ts"
      ]
    }
  },
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": true,
  "files": [
    "dist/**/*.{js,cjs,d.ts,d.cts,css}"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./fs": "./dist/fs/index.js",
    "./types": "./dist/types/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm build",
    "lint": "eslint",
    "clean": "git clean -xdf .turbo dist node_modules",
    "fix": "npm pkg fix"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "sharp": "^0.34.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.7",
    "@portfolio/eslint-config": "workspace:*",
    "@portfolio/prettier-config": "workspace:*",
    "@portfolio/tsconfig": "workspace:*",
    "@swc/core": "^1.11.24",
    "@swc/wasm": "^1.11.24",
    "@types/node": "^22.15.15",
    "chokidar": "^4.0.3",
    "dotenv-cli": "^8.0.0",
    "dotenv-expand": "^12.0.2",
    "eslint": "latest",
    "jiti": "^2.4.2",
    "prettier": "latest",
    "terser": "^5.39.0",
    "tslib": "latest",
    "tsup": "latest",
    "tsx": "latest",
    "typescript": "latest",
    "typescript-eslint": "latest"
  },
  "prettier": "@portfolio/prettier-config"
