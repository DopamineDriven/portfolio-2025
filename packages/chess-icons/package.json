{
  "name": "@portfolio/chess-icons",
  "version": "0.4.0",
  "description": "a minimal react-package shipping chess icon svgs",
  "files": [
    "dist/**/*.{js,mjs,cjs,d.mts,d.ts,d.cts,css,svg}"
  ],
  "license": "MIT",
  "sideEffects": true,
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts",
        "dist/*.d.cts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ],
        "icons": [
          "dist/icons/init.d.ts"
        ],
        "types": [
          "dist/types/index.d.ts"
        ]
      }
    }
  },
  "source": "src/index.ts",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./types": "./dist/types/index.js",
    "./icons": "./dist/icons/index.js",
    "./vectors/*": "./dist/vectors/*.svg"
  },
  "scripts": {
    "lint": "eslint",
    "dev": "pnpm build",
    "prebuild": "rm -rf dist",
    "build": "tsup",
    "postbuild": "cp -r public dist/vectors",
    "clean": "git clean -xdf dist node_modules",
    "types": "tsc --emitDeclarationOnly"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.15"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.50.0",
    "@swc/core": "^1.10.15",
    "@portfolio/eslint-config": "workspace:*",
    "@portfolio/prettier-config": "workspace:*",
    "@portfolio/tsconfig": "workspace:*",
    "@types/node": "^22.13.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "chokidar": "^4.0.3",
    "eslint": "latest",
    "jiti": "^2.4.2",
    "postcss": "^8.5.2",
    "prettier": "latest",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "terser": "^5.38.2",
    "tslib": "latest",
    "tsup": "latest",
    "tsx": "latest",
    "typescript": "latest"
  },
  "prettier": "@portfolio/prettier-config"
}
